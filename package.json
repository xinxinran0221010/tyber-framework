{
  "name": "tyber-framework",
  "version": "1.1.1",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf lib",
    "clean-src": "rm -rf lib/src",
    "clean-test": "find ./test -name '*.js' -type f -exec rm {} {}.map \\; ",
    "lint": "eslint src --fix --ext .ts",
    "src-build": "npx ts-paths build ./ -d -l 2",
    "build:prod": "yarn clean && yarn src-build && npx gulp copy && npx gulp clean-scripts && yarn clean-src",
    "deploy": "npm publish lib",
    "build:test": "tsc --project ./test/tsconfig.test.json",
    "test": "yarn build:test && NODE_ENV=test npx mocha",
    "commit": "git-cz",
    "release": "standard-version"
  },
  "devDependencies": {
    "@testdeck/mocha": "^0.1.2",
    "@types/chai": "^4.2.21",
    "@types/chai-as-promised": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/sinon": "^10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chai": "^4.3.4",
    "chai-as-promised": "^7.1.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "dts-gen": "^0.6.0",
    "eslint": "^7.32.0",
    "gulp": "^4.0.2",
    "gulp-add-src": "^1.0.0",
    "gulp-json-modify": "^1.0.2",
    "mocha": "^9.0.3",
    "nyc": "^15.1.0",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.1.4",
    "ts-node": "^10.2.1"
  },
  "dependencies": {
    "@sentry/node": "^6.10.0",
    "@types/bearcat-es6": "^0.6.0",
    "@types/kcors": "^2.2.4",
    "@types/koa": "^2.13.4",
    "@types/koa-compress": "^4.0.3",
    "@types/koa-json-error": "^3.1.3",
    "@types/koa-morgan": "^1.0.5",
    "@types/koa-qs": "^2.0.0",
    "@types/lodash": "^4.14.172",
    "@types/log4js": "^2.3.5",
    "@types/lru-cache": "^5.1.1",
    "@types/moment-timezone": "^0.5.30",
    "@types/validator": "^13.6.3",
    "bearcat-es6-decorator-aop": "^0.6.6",
    "kcors": "^2.2.2",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-compress": "^5.0.1",
    "koa-json-error": "^3.1.2",
    "koa-morgan": "^1.0.1",
    "koa-qs": "^3.0.0",
    "lodash": "^4.17.21",
    "lru-cache": "^6.0.0",
    "moment-timezone": "^0.5.33",
    "reflect-metadata": "^0.1.13",
    "request": "^2.88.2",
    "standard-version": "^9.3.2",
    "ts-paths": "^0.1.3",
    "typescript": "^4.3.5",
    "validator": "^13.6.0"
  },
  "engines": {
    "node": ">=14.16.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
